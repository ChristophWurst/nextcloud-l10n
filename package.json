{
  "name": "@nextcloud/l10n",
  "version": "1.6.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./dist/gettext": {
      "import": "./dist/esm/gettext.js",
      "require": "./dist/gettext.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "build:doc": "typedoc --excludeNotExported --mode file --out dist/doc lib && touch dist/doc/.nojekyll",
    "check-types": "tsc --noEmit",
    "check-es-compat": "npm run build && check-es-compat dist/*.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud/nextcloud-l10n#readme",
  "author": "Christoph Wurst",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud/nextcloud-l10n"
  },
  "dependencies": {
    "@nextcloud/router": "^2.0.0",
    "core-js": "^3.6.4",
    "dompurify": "^2.4.1",
    "escape-html": "^1.0.3",
    "node-gettext": "^3.0.0"
  },
  "devDependencies": {
    "@happy-dom/jest-environment": "^8.1.0",
    "@nextcloud/browserslist-config": "^2.3.0",
    "@nextcloud/typings": "^1.0.0",
    "@types/node-gettext": "^3.0",
    "check-es-compat": "^2.2.0",
    "gettext-parser": "^4.0.3",
    "jest": "^27.2.1",
    "typedoc": "^0.23.10",
    "typescript": "^4.9.4"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "jest": {
    "testEnvironment": "@happy-dom/jest-environment"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.0.0 || ^8.0.0"
  }
}
